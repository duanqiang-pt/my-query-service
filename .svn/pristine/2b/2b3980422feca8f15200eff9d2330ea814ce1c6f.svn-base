package com.zhongsheng.component.orderby;

import com.zhongsheng.component.enums.OrderByTypeEnum;

/**
 * 排序的详情
 * @version 1.0
 */
public class OrderByDetail {
    
    /*
     * 需要排序的字段名
     */
    private String fieldName;
    
    /*
     * 排序方式
     */
    private OrderByTypeEnum orderByType;

    public String getFieldName() {
        return fieldName;
    }

    public void setFieldName(String fieldName) {
        this.fieldName = fieldName;
    }

    public OrderByTypeEnum getOrderByType() {
        return orderByType;
    }

    public void setOrderByType(OrderByTypeEnum orderByType) {
        this.orderByType = orderByType;
    }

}
